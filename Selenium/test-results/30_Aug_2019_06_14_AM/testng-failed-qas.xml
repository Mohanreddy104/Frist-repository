<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="QAF BDD">
  <test name="BDD Test">
    <method-selectors>
      <method-selector>
        <script language="beanshell">
          <![CDATA[testngMethod.getMethodName().equalsIgnoreCase("New tour register")]]>
        </script>
      </method-selector>
    </method-selectors>
    <parameter name="scenario.file.loc" value="resources/scenarios/Register.bdd"/>
    <classes>
      <class name="com.qmetry.qaf.automation.step.client.text.BDDTestFactory"/>
    </classes>
  </test> <!-- BDD Test -->
</suite> <!-- QAF BDD -->
